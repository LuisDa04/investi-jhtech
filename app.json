{
  "expo": {
    "name": "Investi App",
    "slug": "investi-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "userInterfaceStyle": "light",
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/investi-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/investi-logo.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.investi.app",
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/investi-logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "jsEngine": "hermes",
      "package": "com.investi.app"
    },
    "plugins": [
      "expo-localization",
      "expo-secure-store",
      "expo-router"
    ],
    "scheme": "investi-community",
    "extra": {
      "eas": {
        "projectId": "82b22488-cbbd-45ea-bd0e-dd6ec1f2b7fb"
      }
    },
    "sdkVersion": "51.0.0"
  }
}