{
  "name": "investi-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "dev": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --clear --dev-client",
    "prod": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --clear --no-dev --minify",
    "start:fast": "set RN_NEW_ARCH_ENABLED=0 && set EXPO_USE_TURBO_MODULES=false && expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "test": "jest",
    "reset": "rmdir /s /q node_modules && npm install",
    "clean": "rmdir /s /q .expo .metro node_modules\\.cache"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.2",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/elements": "^1.3.31",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.20",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.59.0",
    "assert": "^2.1.0",
    "babel-preset-expo": "~12.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "expo": "~53.0.0",
    "expo-asset": "~11.0.1",
    "expo-build-properties": "~0.14.1",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.4",
    "expo-font": "~13.0.1",
    "expo-image-picker": "~16.0.3",
    "expo-linking": "~7.0.3",
    "expo-localization": "~16.0.0",
    "expo-router": "~4.0.9",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.28.5",
    "expo-status-bar": "~2.0.0",
    "i18next": "^24.2.0",
    "lucide-react-native": "^0.539.0",
    "nativewind": "^4.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.1",
    "react-i18next": "^15.0.0",
    "react-is": "^18.2.0",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-reanimated": "^3.17.4",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.1.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.19.13",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-react-jsx-source": "^7.27.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.28.2",
    "@playwright/test": "^1.42.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~12.0.0",
    "eslint": "^9.0.0",
    "eslint-config-universe": "^12.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.35",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@sentry/.*|@react-native-community/.*)"
    ]
  }
}
